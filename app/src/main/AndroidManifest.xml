<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sadataljony.app.android.demo.optionmenu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sadataljony.app.android.demo.optionmenu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.sadataljony.app.android.demo.optionmenu.OptionMenuWithLabelActivity"
            android:label="Option Menu With Label" />

        <activity
            android:name="com.sadataljony.app.android.demo.optionmenu.OptionMenuWithIconActivity"
            android:label="Option Menu With Icon" />

        <activity
            android:name="com.sadataljony.app.android.demo.optionmenu.OptionMenuWithListActivity"
            android:label="Option Menu With List" />

    </application>

</manifest>